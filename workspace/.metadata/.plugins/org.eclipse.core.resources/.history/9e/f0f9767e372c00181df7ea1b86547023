package paquete;

import java.util.ArrayList;
public class Nodo {

	private String camino;
	private String resultado;
	private ArrayList<Integer> hijos;
	private Tabla tabla;
	private int padre;
	
	public Nodo () {
		camino = null;
		resultado = null;
		hijos = new ArrayList<Integer>();
		tabla = null;
		padre = -1;
	}
	
	public String getCamino () {
		return camino;
	}
	
	public String getResultado () {
		return resultado;
	}
	
	public ArrayList<Integer> getHijos() {
		return hijos;
	}
	
	public Tabla getTabla () {
		return tabla;
	}
	
	public int getPadre () {
		return padre;
	}
	
	public void setTabla (Tabla t) {
		tabla = t;
	}
}
